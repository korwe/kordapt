<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd ">


   <bean id="genericDAO" abstract="true" class="com.korwe.kordapt.hibernate.AbstractGenericDAO"
              p:sessionFactory-ref="sessionFactory"  />

    <bean id="serviceDAO" parent="genericDAO" class="com.korwe.kordapt.registry.dao.ServiceDAO"/>

    <bean id="serviceInstanceDAO" parent="genericDAO" class="com.korwe.kordapt.registry.dao.ServiceInstanceDAO"/>

</beans>